{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",

  "name": "${mod_name}",
  "description": "Adds elements of the Ninjago franchise into Minecraft",
  "authors": [
    "${mod_author}"
  ],
  "contributors": [
    "waifu_png_pl",
    "Comerciante",
    "Tir-Far-Thoinn",
    "Watommeeeeeeeeee",
    "Albert_1403",
    "ChippiHeppu",
    "Tslat"
  ],
  "contact": {
    "homepage": "https://github.com/thomasglasser/Minejago",
    "issues": "https://github.com/thomasglasser/Minejago/issues"
  },

  "license": "All Rights Reserved",
  "icon": "assets/minejago/logo_1.0.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.thomasglasser.minejago.MinejagoFabric"
    ],
    "client": [
      "dev.thomasglasser.minejago.client.MinejagoFabricClient"
    ],
    "cardinal-components": [
      "dev.thomasglasser.minejago.world.level.storage.MinejagoFabricEntityComponents"
    ],
    "jade": [
      "dev.thomasglasser.minejago.client.MinejagoWailaPlugin"
    ]
  },
  "mixins": [
    "minejago_fabric.mixins.json",
    "minejago_common.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "minecraft": ">=${minecraft_version}",
    "cardinal-components": ">=${cca_version}",
    "smartbrainlib": ">=${sbl_version}",
    "geckolib": ">=${geckolib_version}",
    "sherdsapi": ">=${sherdsapi_version}",
    "forgeconfigapiport": ">=${forgeconfigapiport_version}",
    "trimmed": ">=${trimmed_version}"
  },
  "recommends": {
    "reach-entity-attributes": ">=${reachentityattributes_version}"
  },
  "suggests": {
    "player-animator": ">=${playeranimator_version}",
    "lambdynamiclights": ">=${lambdynamiclights_version}",
    "firstpersonmod": "*",
    "modmenu": "*",
    "configured": ">=${configured_version}",
    "tslatentitystatus": ">=${tes_version}",
    "jade": ">=${jade_version}"
  },

  "accessWidener" : "minejago.accesswidener",

  "custom": {
    "cardinal-components": [
      "minejago:power",
      "minejago:spinjitzu"
    ]
  }
}
  